<odoo>
    <data>
        <record id="seq_proforma" model="ir.sequence">
            <field name="name">Proforma Invoice for Sale Order</field>
            <field name="code">sbk_invoice_report.proforma</field>
            <field name="prefix">PI/%(range_year)s/%(roman_month)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_quotation" model="ir.sequence">
            <field name="name">Quotation</field>
            <field name="code">sbk_invoice_report.quotation</field>
            <field name="prefix">Q/%(range_year)s/%(roman_month)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>