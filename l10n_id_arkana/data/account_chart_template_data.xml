<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="base.IDR" model="res.currency">
        <field name="active" eval="True" />
    </record>
    <record id="transfer_account" model="account.account.template">
        <field name="name">Ayat Silang</field>
        <field name="code">1010701</field>
        <field name="reconcile" eval="True"/>
        <field  name="user_type_id" ref="account.data_account_type_current_assets"/>
    </record>
    <!-- Account Chart template -->
    <record id="l10n_id_chart_template" model="account.chart.template">
        <field name="name">Indonesia - Chart of Accounts</field>
        <field name="bank_account_code_prefix">101010</field>
        <field name="cash_account_code_prefix">101011</field>
        <field name="transfer_account_id" ref="transfer_account"/>
        <field name="code_digits">7</field>
        <field name="currency_id" ref="base.IDR" />
    </record>
    <record id="transfer_account" model="account.account.template">
        <field name="chart_template_id" ref="l10n_id_chart_template"/>
    </record>
</odoo>
