<odoo>
    <data>
        <record model="ir.ui.view" id="product_template_form_view_inherit">
            <field name="name">Document Layout Inherit</field>
            <field name="model">base.document.layout</field>
            <field name="inherit_id" ref="web.view_base_document_layout"/>
            <field name="arch" type="xml">
                <xpath expr="//form[1]/group[1]/group[1]/field[@name='company_details']" position="after">
                    <field name="company_details" string="Company Details 2" options="{'resizable': false}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
