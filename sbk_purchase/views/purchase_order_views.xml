<odoo>
	<data>
		<record model="ir.ui.view" id="purchase_order_form_inherit">
            <field name="name">purchase.order.form.inherit</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[2]//field[@name='date_planned']" position="replace">
                    <field name="date_planned" invisible='1'/>
                </xpath>
                <xpath expr="//page/field[@name='order_line']/tree/field[@name='name']" position="after">
                    <field name="original_name" groups="sbk_product_naming.group_see_origin_name"/>
                </xpath>

                <xpath expr="//field[@name='picking_type_id']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
            </field>
        </record>

