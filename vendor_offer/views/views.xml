<odoo>
  <data>
    <record id="view_partner_form_inherited" model="ir.ui.view">
	<field name="name">Partner Form Inherited</field>
	<field name="model">res.partner</field>
	<field name="inherit_id" ref="base.view_partner_form" />
	<field name="arch" type="xml">
		<page name="internal_notes" position="after">
		    	<page string="Purchase Offers" name="purchase" >
				<field name="purchase_offers" /> 
                    	</page>
		</page>
	</field>
    </record>

    <record id="product_supplierinfo_form_view_inherited" model="ir.ui.view">
    	<field name="name">product.supplierinfo.form.view.inherited</field>
	<field name="model">product.supplierinfo</field>
	<field name="inherit_id" ref="product.product_supplierinfo_form_view" />
	<field name="arch" type="xml">
		<field name="product_id" position="before">
			<field name="product_tmpl_id" string="Product"/>
		</field>
	</field>
    </record>
  </data>
</odoo>
