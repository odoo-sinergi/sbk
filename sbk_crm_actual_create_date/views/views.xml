<odoo>
  <data>
    <record id="crm_case_tree_view_oppor_actual_create_date" model="ir.ui.view">
            <field name="name">crm.lead.tree.opportunity.actual.create.date</field>
            <field name="model">crm.lead</field>
            <field name="priority">20</field>
            <field name="inherit_id" ref="crm.crm_case_tree_view_oppor"/>
            <field name="arch" type="xml">
              <xpath expr="//field[@name='create_date']" position="after">
                <field name="actual_create_date"/>
              </xpath>
            </field>
    </record>

    <record id="crm_case_form_view_oppor_actual_create_date" model="ir.ui.view">
            <field name="name">crm.lead.form.opportunity.actual.create.date</field>
            <field name="model">crm.lead</field>
            <field name="priority">20</field>
            <field name="inherit_id" ref="crm.crm_case_form_view_oppor"/>
            <field name="arch" type="xml">
              <xpath expr="//field[@name='date_deadline']" position="before">
                <field name="actual_create_date"/>
              </xpath>
            </field>
            </record>

  </data>
</odoo>

