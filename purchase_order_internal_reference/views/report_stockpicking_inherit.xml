<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <template id="purchase_report_picking_inherit" inherit_id="stock.report_picking">
            <xpath expr="//t[@align='left' and contains(text(), 'Source')]" position="after">
                <t t-if="o.picking_type_id.code == 'incoming'">Internal Reference</t>
            </xpath>
            <xpath expr="//tr[@t-foreach='move.move_line_ids.sorted(key=lambda ml: ml.location_id.id)' and @t-as='ml']/td[1]/t" position="after">
                <!-- Add your fields here -->
                <t t-if="o.picking_type_id.code == 'incoming'">
                    <span t-esc="ml.product_default_code"/>
                </t>
            </xpath>
        </template>
    </data>
</odoo>