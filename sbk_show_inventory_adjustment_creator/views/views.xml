<odoo>
  <data>
    <record id="view_inventory_form_show_editor" model="ir.ui.view">
        <field name="name">stock.inventory.form.show.editor</field>
        <field name="model">stock.inventory</field>
        <field name="inherit_id" ref="stock.view_inventory_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date']" position="before">
                <field name="user_id"/>
              </xpath>
            </field>
        </record>
  </data>
</odoo>

